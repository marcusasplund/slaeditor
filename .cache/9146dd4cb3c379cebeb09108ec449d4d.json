e":1,"column":3616}},{"source":"index.js","original":{"line":141,"column":35},"generated":{"line":1,"column":3620}},{"source":"index.js","original":{"line":141,"column":35},"generated":{"line":1,"column":3621}},{"source":"index.js","original":{"line":141,"column":40},"generated":{"line":1,"column":3626}},{"source":"index.js","original":{"line":141,"column":32},"generated":{"line":1,"column":3672}},{"source":"index.js","original":{"line":145,"column":0},"generated":{"line":1,"column":3684}},{"source":"index.js","name":"state","original":{"line":145,"column":4},"generated":{"line":1,"column":3686}},{"source":"index.js","name":"state","original":{"line":145,"column":4},"generated":{"line":1,"column":3688}},{"source":"index.js","name":"state","original":{"line":145,"column":4},"generated":{"line":1,"column":3693}},{"source":"index.js","name":"actions","original":{"line":145,"column":11},"generated":{"line":1,"column":3695}},{"source":"index.js","name":"view","original":{"line":145,"column":20},"generated":{"line":1,"column":3697}},{"source":"index.js","name":"document","original":{"line":145,"column":26},"generated":{"line":1,"column":3699}},{"source":"index.js","name":"body","original":{"line":145,"column":35},"generated":{"line":1,"column":3708}}],"sources":{"index.js":"/* global btoa Event */\nimport { h, app } from 'hyperapp'\nimport debounce from 'lodash.debounce'\nimport './styles/app.scss'\n\n/** @jsx h */\nconst state = {\n  parsed: ``,\n  code: '',\n  example: `<html>\n    <head>\n      <style>\n      html {\n        line-height: 1;\n      }\n      body {\n        align-items: center;\n        background-color: #111;\n        display: flex;\n        font-family: Helvetica Neue, sans-serif;\n        height: 100vh;\n        justify-content: center;\n        margin: 0;\n        padding: 0;\n        text-align: center;\n      }\n      h1 {\n        color: #00caff;\n        font-weight: 100;\n        font-size: 8em;\n        margin: 0;\n        padding-bottom: 15px;\n      }\n      button {\n        background: #111;\n        border-radius: 0px;\n        border: 1px solid #00caff;\n        color: #00caff;\n        font-size: 2em;\n        font-weight: 100;\n        margin: 0;\n        outline: none;\n        padding: 5px 15px;\n        transition: background .2s;\n      }\n      button:hover, button:active, button:disabled {\n        background: #00caff;\n        color: #111;\n      }\n      button:active {\n        outline: 2px solid #00caff;\n      }\n      button:focus {\n        border: 1px solid #00caff;\n      }\n      button + button {\n        margin-left: 3px;\n      }\n      </style>\n      </head>\n      <body>\n        <script src=\"https://unpkg.com/hyperapp@1.2.9/dist/hyperapp.js\"></script>\n        <script>\n        const h = hyperapp.h\n        const app = hyperapp.app\n        const state = {\n          count: 0\n        }\n        const actions = {\n          down: value => state => ({ count: state.count - value }),\n          up: value => state => ({ count: state.count + value })\n        }\n        const view = (state, actions) =>\n          h(\"div\", {}, [\n            h(\"h1\", {}, state.count),\n            h(\"button\", { onclick: () => actions.down(1) }, \"-\"),\n            h(\"button\", { onclick: () => actions.up(1) }, \"+\")\n          ])\n        app(state, actions, view, document.body)\n      </script>\n    </body>\n  </html>`,\n  placeholder: 'Paste your awesome website/app code here'\n}\n\nconst actions = {\n  set: x => x,\n  parseString: str => state => ({\n    parsed: btoa(str)\n  }),\n  copyExampleCode: () => (state, actions) => {\n    actions.set({\n      code: state.example\n    })\n    let event = new Event('input')\n    document.getElementById('codearea').dispatchEvent(event)\n  }\n}\n\nconst TextArea = ({ state, parse }) => (\n  <textarea\n    id='codearea'\n    oninput={debounce(parse, 200)}\n    placeholder={state.placeholder}>\n    {state.code}\n  </textarea>\n)\n\nconst view = (state, actions) => (\n  <div id='editor'>\n    <TextArea state={state} parse={e => actions.parseString(e.target.value)} />\n    <div>\n      <a href={`data:text/html;base64, ${state.parsed}`}>\n        <p>\n          This is a link you in some browsers can right click and open in a new tab*<br />\n        </p>\n      </a>\n      <p>\n        <small>*Due to browser restrictions it does not work to open such a link in the same window</small>\n      </p>\n      <hr />\n      <p>\n        Below is your app as a base64-encoded link that you can copy and paste in your browsers adress bar\n      </p>\n      <pre>\n        <code>\n        data:text/html;base64, {state.parsed}\n        </code>\n      </pre>\n      <hr />\n      <p>\n        To try it out; copy the example code below, paste in the left pane, then copy the base64 result produced above and paste in a browser address bar\n        <button disabled={state.parsed} onclick={e => actions.copyExampleCode()}>copy example to textarea</button>\n      </p>\n      <pre>\n        <code>\n          {state.example}\n        </code>\n      </pre>\n    </div>\n    <p>You can look at the code <a href='https://github.com/marcusasplund/slaeditor'>here</a></p>\n  </div>\n)\n\napp(state, actions, view, document.body)\n"},"lineCount":null}},"hash":"c90dc75c87af7e8b5b7902627a9e676a","cacheData":{"env":{}}}